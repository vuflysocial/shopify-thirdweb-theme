<div
  class="claim"
  data-contract-address="{{ section.settings.contractAddress }}"
  data-token-id="{{ section.settings.tokenId }}"
  data-chain-name="{{ section.settings.chainName }}"
></div>

{% schema %}
{
  "name": "Claim Block",
  "settings": [
    {
      "id": "contractAddress",
      "label": "Contract Address",
      "type": "text",
      "default": "0x3f4f45A4C71030F5A1fd2B4cBA5F93fe308Fc8d1"
    },
    {
      "id": "tokenId",
      "label": "Token ID",
      "type": "text",
      "default": "0"
    },
    {
      "type": "text",
      "id": "chainName",
      "label": "Chain Name",
      "default": "binance"
    }
  ],
  "presets": [
    {
      "category": "React Components",
      "name": "Claim Block"
    }
  ]
}
{% endschema %}
